@model Waedist.Models.Contactu

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_AdminDashboardLayout.cshtml";
}


<h3 class="text-danger">Are you sure you want to delete this Massege?</h3>
<div>

    <hr />
    <dl class="row">

        <dd style="font-size:larger; font-weight:bold" class="col-sm-10">
            @Html.DisplayFor(model => model.Subject)
        </dd>

        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Message)

        </dd>
        <dd style="font-size:smaller; font-weight:bold" class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dd style="font-size:smaller; " class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Contid" />
        <input style="border-radius:20px;" type="submit" value="Delete" class="btn btn-danger" /> |
        <a style="color: #eeb7c3" asp-controller="AdminDashboard" asp-action="Messages">Back to List</a>
    </form>
</div>
